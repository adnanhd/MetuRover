<launch>
  <group ns='metu_rover'>
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization" clear_params="true">
      <rosparam command="load" file="$(find leo_rover_localization)/params/ekf_config.yaml" />
    </node>
    <node name='localiation_filter' pkg='leo_rover_localization' type='localization_node.py' />

    <include file="$(find leo_rover_localization)/launch/robot_controller.launch" />
  </group>
</launch>
